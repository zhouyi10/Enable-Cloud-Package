            <groupId>com.enableets.edu.package</groupId>
            <artifactId>enable-package-etm</artifactId>
            <version>1.0.0.release</version>
        </dependency>

        <dependency>
            <groupId>com.enableets.edu.sdk</groupId>
            <artifactId>sdk-teaching-assistant</artifactId>
            <version>1.0.0.RELEASE</version>
        </dependency>

        <dependency>
            <groupId>com.enableets.edu.sdk</groupId>
            <artifactId>sdk-assessment</artifactId>
            <version>1.0.0.RELEASE</version>
        </dependency>
        <dependency>
            <groupId>com.enableets.edu.sdk</groupId>
            <artifactId>sdk-group</artifactId>
            <version>1.0.0.RELEASE</version>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
            </plugin>
        </plugins>
    </build>

</project>
