            <artifactId>enable-package-ppr</artifactId>
            <version>1.0.0.release</version>
        </dependency>

        <dependency>
            <groupId>com.enableets.edu.sdk.catalog</groupId>
            <artifactId>sdk-catalog-v2</artifactId>
            <version>1.0.0.RELEASE</version>
        </dependency>

