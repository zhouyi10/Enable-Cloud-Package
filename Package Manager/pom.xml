        <dependency>
            <groupId>com.enableets.edu.sdk</groupId>
            <artifactId>sdk-school3-v2</artifactId>
            <version>1.1.1</version>
        </dependency>

        <dependency>
            <groupId>com.enableets.edu.sdk</groupId>
            <artifactId>sdk-school3</artifactId>
            <version>1.0.7</version>
        </dependency>

        <dependency>
            <groupId>com.enableets.edu.sdk</groupId>
            <artifactId>sdk-file-storage-v2</artifactId>
