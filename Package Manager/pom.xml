    <dependencies>
        <dependency>
            <groupId>com.enableets.edu.package</groupId>
            <artifactId>package-service</artifactId>
            <version>0.0.1</version>
        </dependency>

        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-configuration-processor</artifactId>
